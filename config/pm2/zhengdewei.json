{
  "apps" : [
    {
      "name"      : "nodeAndExpressDemo",
      "cwd"       : "/D/nodeAndExpressDemo",
      "script"    : "run.js",
      "exec_mode" : "cluster",
      "node_args" : "--harmony",
      "watch"     : ["server"],
      "env"       : {
        "NODE_ENV": "development.zdw"
      },
      "log_date_format" : "YYYY-MM-DD HH:mm Z",
      "error_file"      : "/logs/error.log",
      "out_file"        :   "/logs/out.log",
      "pid_file"        :   "/logs/pid.log",
      "max_memory_restart":   "400M",
      "instances":   "1"
    }
  ]
}
